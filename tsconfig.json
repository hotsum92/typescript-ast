{
  "compilerOptions": {
    "outDir": "./bin/",
    "rootDir": "./lib/",
    "jsx": "react",
    "skipLibCheck": true,
    "sourceMap": false,
    "noImplicitAny": true,
    "strict": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "downlevelIteration": true
  },
  "include": ["lib/**/*"]
}
